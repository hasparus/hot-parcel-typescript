{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["es6", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "react"
  },
  "exclude": ["node_modules", "build", "scripts", "webpack", "jest"]
}
